{
  "name": "rx-web-js",
  "version": "0.0.5",
  "description": "",
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "scripts": {
    "build": "rm -rf dist/* npm run lint && babel src --ignore __tests__ --out-dir dist",
    "build:dev": "webpack --config webpack/dev.config.js",
    "build:prod": "webpack --config webpack/prod.config.js",
    "lint": "eslint src",
    "test": "npm run check && npm run build && npm run testonly",
    "testonly": "mocha --require resources/mocha-bootload src/**/__tests__/**/*.js",
    "check": "flow check",
    "check-cover": "for file in {src/*.js,src/**/*.js}; do echo $file; flow coverage $file; done",
    "build:browser:umd": "cross-env BROWSER=1 webpack --config webpack/umd.config.js",
    "build:node:umd": "cross-env NODE=1 webpack --config webpack/umd.config.js",
    "watch": "node resources/watch.js",
    "test:browser": "cross-env BROWSER=1 ./node_modules/.bin/babel-node test"
  },
  "keywords": [
    "rxjs",
    "redux",
    "reactive",
    "functional"
  ],
  "author": "Henry Tao",
  "license": "BSD-3-Clause",
  "dependencies": {
    "koa": "^2.0.0-alpha.7",
    "koa-router": "^7.0.1",
    "lodash": "^4.17.3",
    "redux": "^3.6.0",
    "rxjs": "^5.0.2",
    "should": "^11.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "cross-env": "^3.1.3",
    "eslint": "3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-flowtype": "2.29.1",
    "flow-bin": "^0.37.4",
    "iflow-koa": "^1.1.21",
    "iflow-koa-bodyparser": "^1.0.6",
    "iflow-koa-router": "^1.1.21",
    "mocha": "3.2.0",
    "moment": "^2.17.1",
    "supertest": "2.0.1"
  }
}
