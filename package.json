{
  "name": "rx-web-js",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "options": {
    "mocha": "--require resources/mocha-bootload src/**/__tests__/**/*.js"
  },
  "scripts": {
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "mocha $npm_package_options_mocha",
    "lint": "eslint src",
    "check": "flow check",
    "build": "rm -rf dist/* && babel src --ignore __tests__ --out-dir dist",
    "build:browser:umd": "cross-env BROWSER=1 webpack --config webpack/umd.config.js",
    "build:node:umd": "cross-env NODE=1 webpack --config webpack/umd.config.js",
    "watch": "node resources/watch.js"
  },
  "keywords": [
    "rxjs",
    "reactive",
    "functional"
  ],
  "author": "Henry Tao",
  "license": "BSD-3-Clause",
  "dependencies": {
    "koa": "^2.0.0-alpha.7",
    "koa-router": "^7.0.1",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "cross-env": "^3.1.3",
    "eslint": "3.12.2",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-flowtype": "2.29.1",
    "flow-bin": "^0.37.4",
    "iflow-koa": "^1.1.21",
    "iflow-koa-bodyparser": "^1.0.6",
    "iflow-koa-router": "^1.1.21",
    "mocha": "3.2.0",
    "moment": "^2.17.1",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2"
  }
}
